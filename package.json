{
  "name": "mocha-marble-bdd",
  "version": "1.3.41",
  "description": "Mocha BDD UI extended for RxJs5 marble testing",
  "license": "MIT",
  "author": "Barry Kaplan <barry@systeminsights.com>",
  "homepage": "https://github.com/systeminsights/mocha-marble-bdd",
  "keywords": [
    "mocha",
    "test",
    "bdd",
    "rxjs"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/systeminsights/mocha-marble-bdd.git"
  },
  "publishConfig": {
    "registry": "https://nexus.systeminsights.com/repository/si-npm"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "engines": {
    "node": ">=6.9.1"
  },
  "scripts": {
    "test": "mocha --reporter dot --ui marble-bdd --require @babel/register --require ./src/index.js --recursive ./test",
    "tc:eslint": "echo 'ESlint not enforced because code was copied from mocha project'",
    "tc:coverage": "tc-coverage-mocha",
    "tc:build": "tc-build"
  },
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "chai": "4.2.0",
    "escape-string-regexp": "2.0.0",
    "mocha": "7.1.0",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "babel-preset-vi-node": "1.0.137",
    "eslint-config-vi-node": "1.2.202"
  },
    "mocha": {
      "require": [
        "@babel/register"
      ],
      "timeout": 5000,
      "ui": "./src/index.js"
    }
}
