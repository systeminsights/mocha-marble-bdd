{
  "name": "mocha-marble-bdd",
  "version": "1.2.103",
  "description": "Mocha BDD UI extended for RxJs5 marble testing",
  "license": "MIT",
  "author": "Barry Kaplan <barry@systeminsights.com>",
  "homepage": "https://github.com/systeminsights/mocha-marble-bdd",
  "keywords": [
    "mocha",
    "test",
    "bdd",
    "rxjs"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/systeminsights/mocha-marble-bdd.git"
  },
  "publishConfig": {
    "registry": "https://nexus.systeminsights.com/repository/si-npm"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "scripts": {
    "test": "mocha --ui marble-bdd --require index.js --recursive ./test",
    "tc:version": "node version",
    "tc:eslint": "echo 'ESlint not enforced because code was copied from mocha project'",
    "tc:test": "mocha --require @babel/register --ui marble-bdd --reporter mocha-teamcity-reporter --require index.js --recursive ./test",
    "tc:coverage": "NODE_ENV=test ELASTIC_APM_ACTIVE=false nyc --reporter=lcov --reporter=teamcity mocha --require @babel/register --ui marble-bdd --require index.js --recursive ./test",
    "tc:build": "babel index.js -d dist; babel src -d dist/src"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@babel/core": "7.6.4",
    "chai": "4.2.0",
    "escape-string-regexp": "2.0.0",
    "mocha": "6.2.1",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/register": "7.6.2",
    "babel-plugin-istanbul": "5.2.0",
    "eslint-config-vi-node": "1.2.95",
    "mocha-teamcity-reporter": "3.0.0",
    "nyc": "14.1.1",
    "semver": "6.3.0"
  }
}
