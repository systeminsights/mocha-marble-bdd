{
  "name": "mocha-marble-bdd",
  "version": "1.2.87",
  "description": "Mocha BDD UI extended for RxJs5 marble testing",
  "license": "MIT",
  "author": "Barry Kaplan <barry@systeminsights.com>",
  "homepage": "https://github.com/systeminsights/mocha-marble-bdd",
  "keywords": [
    "mocha",
    "test",
    "bdd",
    "rxjs"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/systeminsights/mocha-marble-bdd.git"
  },
  "publishConfig": {
    "registry": "https://repo.systeminsights.com/repository/si-npm"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "scripts": {
    "test": "mocha --ui marble-bdd --require index.js --recursive ./test",
    "tc:version": "node version",
    "tc:test_pre": "echo test_pre noop",
    "tc:test": "mocha --compilers js:babel-register --ui marble-bdd --reporter mocha-teamcity-reporter --require index.js --recursive ./test",
    "tc:coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=teamcity mocha --compilers js:babel-register --ui marble-bdd --require index.js --recursive ./test",
    "tc:test_post": "echo test_post noop",
    "tc:build": "babel index.js -d dist; babel src -d dist/src"
  },
  "main": "dist/index.js",
  "dependencies": {
    "babel-core": "6.26.3",
    "chai": "4.1.2",
    "escape-string-regexp": "1.0.5",
    "mocha": "5.2.0",
    "rxjs": "6.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-istanbul": "5.0.1",
    "babel-preset-env": "1.7.0",
    "mocha-teamcity-reporter": "2.5.0",
    "nyc": "13.0.1",
    "semver": "5.5.1"
  }
}
