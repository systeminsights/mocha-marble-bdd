    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@babel/node": "7.7.0",
    "babel-preset-vi-node": "1.0.113",
    "eslint-config-vi-node": "1.2.161",
    "mocha-marble-bdd": "1.2.110",
    "mocha-teamcity-reporter": "3.0.0",
    "nyc": "14.1.1",
    "semver": "6.3.0"
  }
